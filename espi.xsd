	<xs:element name="CommodityType" type="CommodityType"/>
	<xs:element name="ConsumptionTierType" type="ConsumptionTierType"/>
	<xs:element name="CurrencyCode" type="CurrencyCode"/>
	<xs:element name="DataQualifierType" type="DataQualifierType"/>
	<xs:element name="DateTimeInterval" type="DateTimeInterval"/>
	<xs:element name="FlowDirectionType" type="FlowDirectionType"/>
	<xs:element name="KindType" type="KindType"/>
