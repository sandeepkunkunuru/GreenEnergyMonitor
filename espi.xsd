	<xs:element name="QualityOfReading" type="QualityOfReading"/>
	<xs:element name="ServiceKind" type="ServiceKind"/>
	<xs:element name="TimeAttributeType" type="TimeAttributeType"/>
	<xs:element name="TOUType" type="TOUType"/>
	<xs:element name="UomType" type="UomType"/>
	<xs:element name="SummaryMeasurement" type="SummaryMeasurement"/>
	<xs:element name="BatchItemInfo" type="BatchItemInfo"/>
	<xs:element name="Object" type="Object"/>
